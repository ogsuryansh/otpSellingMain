{
  "version": 2,
  "builds": [
    {
      "src": "vercel-server-simple.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/styles/(.*)",
      "destination": "/public/styles/$1"
    },
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/vercel-server-simple.js"
    }
  ]
}
